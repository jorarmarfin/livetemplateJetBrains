useFormCustom

import {useForm} from "react-hook-form";

export const useFormCaloriesTracker = () => {
    const {
        register,
        handleSubmit
    } = useForm();
    const onSubmit = (data) => {
        console.log(data);
    }

    return {
        register,
        handleSubmit,
        onSubmit
    }
}

